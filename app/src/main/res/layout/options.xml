        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="top"
            android:layout_gravity="top"
            android:orientation="vertical" >



            <TextView
                android:layout_marginTop="5dp"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_gravity="center"
                android:layout_centerInParent="true"
                android:textSize="25dp"
                android:textColor="#b666d2"
                android:background="@drawable/newlogo"
                android:gravity="center_horizontal"></TextView>
            <TextView
                android:layout_marginTop="30dp"
                android:layout_marginLeft="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="EID,VL lab results"
                android:textSize="15dp"
                android:textColor="#000"
                android:gravity="center"></TextView>



            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="goToResults"
                android:textColor="#b666d2"
                android:textAllCaps="false"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="9dp"
                android:background="@drawable/button"
                android:text="Results"
                android:id="@+id/button" />


            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="HTSResults"
                android:textColor="#b666d2"
                android:textAllCaps="false"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="9dp"
                android:background="@drawable/button"
                android:text="HTS Results"
                android:id="@+id/htsbutton" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:onClick="goToDb"
                android:textColor="#b666d2"
                android:textAllCaps="false"
                android:background="@drawable/button"
                android:text="Dashboard"/>




            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:onClick="goToClients"
                android:textColor="#b666d2"
                android:textAllCaps="false"
                android:background="@drawable/button"
                android:text="Add Client"/>


            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:onClick="sampleLogin"
                android:textColor="#b666d2"
                android:textAllCaps="false"
                android:background="@drawable/button"
                android:text="Sample Remote Login"/>


            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:onClick="historicalResults"
                android:textColor="#b666d2"
                android:textAllCaps="false"
                android:background="@drawable/button"
                android:text="Historical Results"/>

            <!--<Button-->
                <!--android:layout_width="match_parent"-->
                <!--android:layout_height="wrap_content"-->
                <!--android:layout_marginTop="10dp"-->
                <!--android:layout_marginLeft="4dp"-->
                <!--android:onClick="messaging"-->
                <!--android:textColor="#b666d2"-->
                <!--android:textAllCaps="false"-->
                <!--android:background="@drawable/button"-->
                <!--android:text="Messaging"/>-->

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="horizontal">


            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/mohkenya"
                android:padding="4dp"
                android:onClick="gomoh"
                android:id="@+id/imageView2"
                android:layout_weight="1"/>


            <!--<ImageView-->
                <!--android:layout_width="match_parent"-->
                <!--android:layout_height="match_parent"-->
                <!--android:onClick="gocdc"-->
                <!--android:src="@drawable/cdc"-->
                <!--android:padding="4dp"-->
                <!--android:layout_weight="1"-->
                <!--android:id="@+id/imageView3"></ImageView>-->

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/pepfarkenya"
                android:layout_weight="1"
                android:onClick="gopepfar"
                android:padding="4dp"
                android:id="@+id/imageView4">

            </ImageView>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/mhealthlogotwo"
                android:id="@+id/imageView"
                android:padding="4dp"
                android:onClick="gomhealth"
                android:layout_weight="1"/>


        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_gravity="center_horizontal"
            android:gravity="center_horizontal"
            android:text="version"
            android:textStyle="italic"
            android:id="@+id/version"
            android:layout_height="wrap_content" />


    </LinearLayout>
</ScrollView>








